<p id="notice"><%= notice %></p>

<h1>Listing Entity Tokens</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entity_tokens.each do |entity_token| %>
      <tr>
        <td><%= link_to 'Show', entity_token %></td>
        <td><%= link_to 'Edit', edit_entity_token_path(entity_token) %></td>
        <td><%= link_to 'Destroy', entity_token, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entity token', new_entity_token_path %>
